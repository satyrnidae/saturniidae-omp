{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#e1c24a",
          "background": "transparent",
          "properties": {
            "root_icon": ""
          }
        },
        {
          "type": "os",
          "style": "powerline",
          "background": "#684428",
          "foreground": "#e3e1ba",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "style": "mixed"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "#b66528",
          "foreground": "#e3e1ba"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "#e1c24a",
          "foreground": "#353827",
          "properties": {
            "style": "full",
            "folder_separator_icon": "/"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "#a5be59",
          "foreground": "#353827",
          "properties": {
            "display_stash_count": true,
            "working_color": "#7e271b",
            "staging_color": "#455023"
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "background": "#e3e1ba",
          "foreground": "#353827",
          "powerline_symbol": "",
          "properties": {
            "display_mode": "context"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "background": "#353827",
          "foreground": "#e3e1ba",
          "properties": {
            "error_color": "#7e271b",
            "color_background": true,
            "always_enabled": false,
            "display_exit_code": true,
            "success_icon": "<green>\u2713</>",
            "error_icon": "\u2717 "
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "background": "#353827",
          "foreground": "#e3e1ba",
          "properties": {
            "style": "amarillo",
            "threshold": 100
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#e3e1ba",
          "properties": {
            "postfix": " > "
          }
        }
      ]
    }
  ]
}