{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "root_icon": "\ue0a2"
          },
          "template": " \uf0e7 ",
          "foreground": "#e1c24a",
          "background": "transparent",
          "type": "root",
          "style": "plain"
        },
        {
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#e3e1ba",
          "powerline_symbol": "\ue0b6",
          "leading_powerline_symbol": "\ue0b6",
          "background": "#703F2D",
          "type": "os",
          "style": "powerline",
          "invert_powerline": false
        },
        {
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#e3e1ba",
          "powerline_symbol": "\ue0b4",
          "background": "#B56528",
          "type": "session",
          "style": "powerline"
        },
        {
          "options": {
            "folder_icon": "..",
            "hide_root_location": false,
            "max_depth": 3,
            "style": "agnoster",
            "folder_separator_icon": "/"
          },
          "template": " {{ .Path }} ",
          "foreground": "#32140D",
          "powerline_symbol": "\ue0b4",
          "background": "#E0C24A",
          "type": "path",
          "style": "powerline"
        },
        {
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 <#7e271b>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 <#455023>{{ .Staging.String }}</>{{ end }} ",
          "foreground": "#32140D",
          "powerline_symbol": "\ue0b4",
          "background": "#A6BF5A",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#DC52A0{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#A6BF5A{{ end }}",
            "{{ if gt .Ahead 0 }}#DC52A0{{ end }}",
            "{{ if gt .Behind 0 }}#60C2E0{{ end }}"
          ]
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " \ue71e {{ .Full }} ",
          "foreground": "#32140D",
          "powerline_symbol": "\ue0b4",
          "background": "#e3e1ba",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#32140D",
          "background": "#e3e1ba",
          "template": " \ue73c {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "style": "amarillo",
            "threshold": 100
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#e3e1ba",
          "background": "#414B23",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " {{ .Name }} > ",
          "foreground": "#e3e1ba",
          "type": "shell",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
